<Window x:Class="SklepInternetowyWPF.Views.AccountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Konto użytkownika" Height="300" Width="400">
    <StackPanel Margin="20">
        <TextBlock x:Name="WelcomeText" FontSize="20" FontWeight="Bold" Margin="0,0,0,20"/>

        <Button Content="Historia zamówień" Click="History_Click" Margin="0,5"/>
        <Button Content="Wyloguj się" Click="Logout_Click" Margin="0,5"/>
        <Button Content="Zamknij" Click="Close_Click" Margin="0,5"/>
    </StackPanel>
</Window>
